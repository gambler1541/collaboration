<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
                  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- 마이바티스의 작동 규칙정의 -->
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias alias="vo" type="com.collaboration.colla.vo.MemberVO" />
		<typeAlias alias="bvo" type="com.collaboration.colla.vo.BoardVO" />
		<typeAlias alias="lvo" type="com.collaboration.colla.vo.ListVO" />
		<typeAlias alias="cvo" type="com.collaboration.colla.vo.CardVO" />
		<typeAlias alias="wmvo" type="com.collaboration.colla.vo.WithMemberVO" />
		<typeAlias alias="bmvo" type="com.collaboration.colla.vo.BMemberVO" />
	</typeAliases>
</configuration>